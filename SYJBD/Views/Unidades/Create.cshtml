@model SYJBD.Models.Unidad
@{
    ViewData["Title"] = "Nueva unidad";
}
<div class="card">
    <div class="card-header"><h2 class="card-title">@ViewData["Title"]</h2></div>
    <div class="content" style="padding:16px;">
        <form asp-action="Create" method="post">
            @Html.AntiForgeryToken()
            @await Html.PartialAsync("_Form", Model)
            <div class="mt-3">
                <button class="btn btn-primary">Guardar</button>
                <a asp-action="Index" class="btn">Cancelar</a>
            </div>
        </form>
    </div>
</div>
